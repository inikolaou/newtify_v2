<component name="ProjectRunConfigurationManager">
    <configuration default="false" name="NewtifySpringWebApplication" type="SpringBootApplicationConfigurationType"
                   factoryName="Spring Boot">
        <module name="newtify-web"/>
        <extension name="coverage">
            <pattern>
                <option name="PATTERN" value="sk.best.newtify.web.bootstrap.*"/>
                <option name="ENABLED" value="true"/>
            </pattern>
        </extension>
        <option name="SPRING_BOOT_MAIN_CLASS" value="sk.best.newtify.web.bootstrap.NewtifyWebApplication"/>
        <option name="VM_PARAMETERS" value="-Dvaadin.enableDevServer=true"/>
        <option name="WORKING_DIRECTORY" value="file://$PROJECT_DIR$/newtify-web"/>
        <option name="ALTERNATIVE_JRE_PATH"/>
        <option name="SHORTEN_COMMAND_LINE" value="ARGS_FILE"/>
        <method v="2">
            <option name="Make" enabled="true"/>
        </method>
    </configuration>
</component>